{
	"name": "ffr",
	"$schema": "../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "ffr/src",
	"projectType": "application",
	"targets": {
		"build": {
			"executor": "@nx/webpack:webpack",
			"outputs": ["{options.outputPath}"],
			"defaultConfiguration": "production",
			"options": {
				"compiler": "babel",
				"outputPath": "dist/ffr",
				"index": "ffr/src/index.html",
				"baseHref": "/",
				"main": "ffr/src/main.ts",
				"tsConfig": "ffr/tsconfig.app.json",
				"assets": ["ffr/src/favicon.ico", "ffr/src/assets"],
				"styles": ["ffr/src/styles.css"],
				"scripts": [],
				"webpackConfig": "ffr/webpack.config.ts"
			},
			"configurations": {
				"development": {
					"extractLicenses": false,
					"optimization": false,
					"sourceMap": true,
					"vendorChunk": true
				},
				"production": {
					"fileReplacements": [
						{
							"replace": "ffr/src/environments/environment.ts",
							"with": "ffr/src/environments/environment.prod.ts"
						}
					],
					"optimization": true,
					"outputHashing": "all",
					"sourceMap": false,
					"namedChunks": false,
					"extractLicenses": true,
					"vendorChunk": false,
					"webpackConfig": "ffr/webpack.config.prod.ts"
				}
			}
		},
		"serve": {
			"executor": "@nx/react:module-federation-dev-server",
			"defaultConfiguration": "development",
			"options": {
				"buildTarget": "ffr:build",
				"hmr": true,
				"port": 4205
			},
			"configurations": {
				"development": {
					"buildTarget": "ffr:build:development"
				},
				"production": {
					"buildTarget": "ffr:build:production",
					"hmr": false
				}
			}
		},
		"serve-static": {
			"executor": "@nx/web:file-server",
			"defaultConfiguration": "production",
			"options": {
				"buildTarget": "ffr:build",
				"watch": false,
				"port": 4205
			},
			"configurations": {
				"development": {
					"buildTarget": "ffr:build:development"
				},
				"production": {
					"buildTarget": "ffr:build:production"
				}
			}
		}
	},
	"tags": []
}
